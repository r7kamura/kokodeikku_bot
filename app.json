{
  "description": "Find ikku from twitter timeline.",
  "env": {
    "BUILDPACK_URL": {
      "description": "Buildpack URL",
      "value": "https://github.com/diasks2/heroku-buildpack-mecab"
    },
    "LOG_LEVEL": {
      "description": "Log level for debugging",
      "value": "0"
    },
    "MECAB_PATH": {
      "description": "MeCab path",
      "value": "/app/vendor/mecab/lib/libmecab.so"
    },
    "RUBOTY_NAME": {
      "description": "Bot name"
    },
    "TWITTER_ACCESS_TOKEN": {
      "description": "Twitter access token with read & write permission"
    },
    "TWITTER_ACCESS_TOKEN_SECRET": {
      "description": "Twitter access token secret"
    },
    "TWITTER_AUTO_FOLLOW_BACK": {
      "description": "Pass 1 to follow back followers",
      "value": "1"
    },
    "TWITTER_CONSUMER_KEY": {
      "description": "Twitter consumer key"
    },
    "TWITTER_CONSUMER_SECRET": {
      "description": "Twitter consumer secret"
    }
  },
  "keywords": ["bot", "twitter", "ここで一句"],
  "name": "kokodeikku-bot"
}
